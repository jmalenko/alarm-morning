<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/accent"/>
            <size
                android:width="96dp"
                android:height="96dp"/>
            <padding
