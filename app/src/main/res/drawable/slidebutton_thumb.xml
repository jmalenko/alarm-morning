<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/accent"/>
            <size
                android:width="64dp"
                android:height="64dp"/>
            <padding
                android:bottom="16dp"
                android:left="16dp"
                android:right="16dp"
                android:top="16dp"/>
        </shape>
    </item>

    <item>
        <bitmap
            android:src="@drawable/ic_alarm_off_white"/>
    </item>


</layer-list>