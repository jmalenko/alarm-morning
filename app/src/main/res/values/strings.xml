    <string name="dismiss_alarm_on_device_shake">Dismiss alarm on device shake</string>

    <string name="wizard_set_features_title">Set advanced alarm clock features</string>
    <string name="wizard_set_features_description">Every evening at <xliff:g example="22:00" id="time">%1$s</xliff:g> do…</string>

    <string name="check_alarm_time">Check that the alarm time is set at least 1 hour before the first appointment</string>
    <string name="play_nighttime_bell">Play nighttime bell (taps bugle call)</string>

    <string name="wizard_set_permissions_title">Allow permissions</string>
    <string name="wizard_set_permissions_description">The following dialogues ask for the permissions the alarm clock needs.</string>
    <string name="wizard_set_permissions_explanation"><![CDATA[
<p>Alarm Morning needs the following permissions to provide full user experience:</p>
<p><b>READ_CALENDAR</b> – to read the calendar events (the information about next appointment is shown while the alarm is ringing)</p>
<p><b>WRITE_EXTERNAL_STORAGE</b> – to install new ringtone (the “Church bell” ringtone is used as default sound for nighttime bell)</p>
<p><b>ACCESS_FINE_LOCATION</b> – to get the location (the location determines the holidays in your region)</p>
<p><b>INTERNET</b> – to use network (to update the list of holidays)</p>
]]></string>

