    <string name="title_activity_wizard">First Run Wizard</string>

    <string name="wizard_welcome_title">Welcome to Alarm Morning</string>
    <string name="wizard_welcome_description">We will guide you through a basic setup. All the settings can be changed later.</string>

    <string name="wizard_set_time_title">Set alarm time</string>
    <string name="wizard_set_time_description_top">When do you want the alarm clock to go off on workdays?</string>
    <string name="wizard_set_time_description_bottom">On weekends, the alarm clock will be disabled.</string>

    <string name="wizard_set_holidays_title">Set holidays</string>
    <string name="wizard_set_holidays_description">On holidays, the alarm will be disabled. Select your country.</string>

    <string name="wizard_set_actions_title">Set actions while ringing</string>
    <string name="wizard_set_actions_description">There are also events related to key press and proximity sensor.</string>

    <string name="mute_alarm_for_10_seconds_on_device_move">Mute alarm for 10 seconds on device move</string>
    <string name="snooze_alarm_on_device_flip">Snooze alarm on device flip</string>
    <string name="dismiss_alarm_on_device_shake">Dismiss alarm on device shake</string>

    <string name="wizard_set_features_title">Set advanced alarm clock fetures</string>
    <string name="wizard_set_features_description">Every evening at 10:00 PM do…</string>

    <string name="check_alarm_time">Check that the alarm time is set at least 1 hour before the first appointment</string>
    <string name="play_nighttime_bell">Play nighttime bell</string>

    <string name="wizard_set_permissions_title">Allow permissions</string>
    <string name="wizard_set_permissions_description">The following dialogues ask for the permissions the alarm clock needs.</string>
    <string name="wizard_set_permissions_explanation"><![CDATA[
<p>Alarm Morning needs the following permissions to provide full user experience:</p>
<p><b>READ_CALENDAR</b> – to read the calendar events (the information about next appointment is shown while the alarm is ringing)</p>
<p><b>WRITE_EXTERNAL_STORAGE</b> – to install new ringtone (the “Church bell” ringtone is used as default sound for nighttime bell)</p>
]]></string>

    <string name="wizard_done_title">You are all set</string>
    <string name="wizard_done_description">Sleep well and don\'t worry about alarm clock management.</string>

